# MediVoice AI Frontend Configuration
# Copy this file to .env.local and update with your actual values

# API Configuration
VITE_API_URL=https://your-api-id.execute-api.us-east-1.amazonaws.com/dev

# AWS Configuration
VITE_AWS_REGION=us-east-1

# AWS Cognito Configuration
VITE_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
VITE_COGNITO_APP_CLIENT_ID=XXXXXXXXXXXXXXXXXX
VITE_COGNITO_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
VITE_COGNITO_HOSTED_UI_URL=https://your-domain.auth.us-east-1.amazoncognito.com

# Development Configuration
VITE_PORT=5173

# Feature Flags
VITE_ENABLE_AUTH=true
VITE_ENABLE_REALTIME=false
VITE_ENABLE_ANALYTICS=false

# Debug Settings (automatically enabled in development)
# These are used internally by the config system